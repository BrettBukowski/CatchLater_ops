server {
    listen      80;
    server_name catchlater.com www.catchlater.com;
    if ($host = 'www.catchlater.com') {
        rewrite ^/(.*)$ http://catchlater.com/$1 permanent;
    }
    root /var/catchlater/current/public;

    passenger_enabled on;
    passenger_use_global_queue on;
    passenger_max_requests 10000;
}
